{
  "name": "vscode-lms",
  "displayName": "VSCode-LMS",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.88.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:vscode-lms-sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vscode-lms-sidebar-view",
          "title": "vscode-lms",
          "icon": "media/rocket.svg"
        }
      ]
    },
    "views": {
      "vscode-lms-sidebar-view": [
        {
          "type": "webview",
          "id": "vscode-lms-sidebar",
          "name": "vscode-lms",
          "icon": "media/rocket.svg",
          "contextualTitle": "vscode-lms"
        }
      ]
    },
    "commands": [
      {
        "command": "vscode-lms.helloWorld",
        "title": "Clone Repository Project"
      },
      {
        "command": "vscode-lms.refresh",
        "title": "Refresh"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"rollup -c --bundleConfigAsCjs -w\" \"webpack --watch --config ./build/node-extension.webpack.config.js\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@tsconfig/svelte": "^5.0.4",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.88.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "path": "^0.12.7",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "svelte": "^4.2.16",
    "svelte-check": "^3.7.1",
    "svelte-preprocess": "^5.1.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "simple-git": "^3.24.0"
  }
}
